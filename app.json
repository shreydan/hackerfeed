{
	"expo": {
		"name": "hackerfeed",
		"slug": "hackerfeed",
		"version": "0.5.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffd7f5"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffd7f5"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
